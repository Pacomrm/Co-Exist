er} from \"react-bootstrap\";\n\nexport function Projects(){\n\n    const allProjects = useSelector(selectAllProjects);\n    const dispatch = useDispatch();\n    let display = {};\n\n    useEffect(()=>{\n        dispatch(loadAllProjects());\n    },[])\n\n    if(allProjects.length > 1){\n        display =  <ProjectsList projects={allProjects}/>;\n    }else{\n        display = <p>No projects yet</p>;\n    }\n\n    return (\n        <>\n            <Container>\n                <ProjectNav/>\n                <section className=\"projects-upper-container\">\n                    {display}\n                </section>\n            </Container>\n\n\n\n            {/*<section className=\"projects-bottom-container\">*/}\n            {/*    <h2>Sign in to interact</h2>*/}\n            {/*    <p>Form</p>*/}\n            {/*    <div className=\"projects-signin-form\">*/}\n\n            {/*    </div>*/}\n            {/*</section>*/}\n        </>\n    )\n}\n\n\n// Container for display projects component","/Users/mrm/Documents/TECH/React/Redux/Co-exist/react-express-app/client/src/views/Projects/ProjectNav.js",["70","71","72"],[],"import * as React from \"react\";\nimport {Nav} from \"react-bootstrap\";\nimport {useEffect, useRef, useState} from \"react\";\nimport FilterProjects from '../../services/FilterProjects.js'\nimport {useDispatch} from \"react-redux\";\nimport {loadAllProjects} from \"../../features/projects/projectSlice\";\n\nexport function ProjectNav(){\n    const valueRef = useRef(false);\n    const dispatch = useDispatch();\n    const [filter, setFilter] = useState({\n        ods: false,\n        location: false,\n        needs: false\n    });\n    let navFilter = false;\n\n    function handleClickFilters(e){\n\n        setFilter({\n            ...filter,\n            [e.target.name]: true\n        })\n\n    }\n    function cleanFilters() {\n        setFilter({\n            ods: false,\n            location: false,\n            needs: false\n        });\n        dispatch(loadAllProjects());\n    }\n    const cleanFromChild = (val) => {\n        console.log(\"llego\",val);\n        cleanFilters();\n    }\n\n    return (\n        <section className=\"project-nav\">\n            {/*<p>Filter projects by your preference</p>*/}\n            <Nav className=\"justify-content-center\" activeKey=\"/home\">\n                <Nav.Item>\n                    <Nav.Link name=\"ods\" id=\"byODS\" eventKey=\"link-0\" onClick={handleClickFilters}>by ODS</Nav.Link>\n                </Nav.Item>\n                <Nav.Item>\n                    <Nav.Link name=\"location\" eventKey=\"link-1\" onClick={handleClickFilters}>by Location</Nav.Link>\n                </Nav.Item>\n                <Nav.Item>\n                    <Nav.Link name=\"needs\" eventKey=\"link-2\" onClick={handleClickFilters}>by Needs</Nav.Link>\n                </Nav.Item>\n                <Nav.Item>\n                    <Nav.Link style={{color:'333333'}} name=\"clean\" eventKey=\"link-4\" onClick={cleanFilters}> / start again</Nav.Link>\n                </Nav.Item>\n            </Nav>\n            <FilterProjects type={filter} valueRef={valueRef}/>\n\n        </section>\n    )\n}","/Users/mrm/Documents/TECH/React/Redux/Co-exist/react-express-app/client/src/views/Projects/ProjectsList.js",[],[],"/Users/mrm/Documents/TECH/React/Redux/Co-exist/react-express-app/client/src/views/Projects/Project.js",[],[],"/Users/mrm/Documents/TECH/React/Redux/Co-exist/react-express-app/client/src/services/FilterProjects.js",["73"],[],"import {Nav} from \"react-bootstrap\";\nimport * as React from \"react\";\nimport {useDispatch, useSelector} from \"react-redux\";\nimport {\n    loadAllLocations, loadAllNeeds,\n    loadAllODSs, loadProjectsByFilterArray, loadProjectsByLocation,\n    selectAllLocations, selectAllNeeds,\n    selectAllODS\n} from \"../features/projects/projectSlice\";\nimport {useEffect} from \"react\";\n\nexport default function FilterProjects(filterType){\n\n    const dispatch = useDispatch();\n    const allODS = useSelector(selectAllODS);\n    const allLocations = useSelector(selectAllLocations);\n    const allNeeds = useSelector(selectAllNeeds);\n    let allCategories = '';\n    let displayAllCategories = [];\n\n    if(filterType.type.ods === true){allCategories = allODS}\n    if(filterType.type.location === true){allCategories = allLocations}\n    if(filterType.type.needs === true){allCategories = allNeeds}\n\n    useEffect(()=>{\n        dispatch(loadAllODSs())\n        dispatch(loadAllLocations())\n        dispatch(loadAllNeeds())\n    },[]);\n\n    function handleClickODSFilter(e){\n        console.log(e.target.name);\n        if(filterType.type.location === true){\n            dispatch(loadProjectsByLocation(e.target.name));\n            console.log(e.target.name);\n        }else{\n            dispatch(loadProjectsByFilterArray(e.target.name,filterType.type));\n        }\n\n    }\n    if(allCategories.length > 0 ){\n        allCategories.forEach((o,index) => {\n            // console.log(index);\n            displayAllCategories.push(\n                <Nav.Item key={index}>\n                    <Nav.Link name={o} id={o} eventKey=\"link-0\" onClick={handleClickODSFilter}>{o}</Nav.Link>\n                </Nav.Item>\n            )\n        })\n    }else if(filterType.type !== true){\n        return ;\n        // <Nav.Item><p>No projects available for this filter</p></Nav.Item>;\n    }\n    return (\n        <Nav className=\"justify-content-center\" activeKey=\"/home\">\n            {\n                displayAllCategories\n            }\n        </Nav>\n    )\n}",{"ruleId":"74","severity":1,"message":"75","line":2,"column":16,"nodeType":"76","messageId":"77","endLine":2,"endColumn":19},{"ruleId":"74","severity":1,"message":"78","line":4,"column":7,"nodeType":"76","messageId":"77","endLine":4,"endColumn":19},{"ruleId":"79","severity":1,"message":"80","line":89,"column":39,"nodeType":"81","messageId":"82","endLine":89,"endColumn":41},{"ruleId":"79